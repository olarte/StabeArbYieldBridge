# StableArbYieldBridge Environment Configuration Example
# Copy this to .env and fill in your actual values

# Server Configuration
PORT=3001
NODE_ENV=development

# API Keys (Required for production)
ONEINCH_API_KEY=your_1inch_api_key_here
ALCHEMY_KEY=your_alchemy_api_key_here

# Wallet Private Keys (Required for trading operations)
# IMPORTANT: Keep these secure and never commit to version control
CELO_PRIVATE_KEY=your_celo_testnet_private_key_here
SUI_PRIVATE_KEY=your_sui_testnet_private_key_here

# RPC Endpoints (Testnet configuration)
ETHEREUM_RPC=https://eth-sepolia.g.alchemy.com/v2/your_alchemy_key
CELO_RPC=https://alfajores-forno.celo-testnet.org
SUI_RPC=https://fullnode.testnet.sui.io:443

# Database Configuration (if using PostgreSQL)
# DATABASE_URL=postgresql://username:password@localhost:5432/stablearb

# Security Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Development Settings
DEBUG=true
LOG_LEVEL=info

# Bridge Configuration
MIN_ARBITRAGE_PROFIT=0.5
MAX_SLIPPAGE_TOLERANCE=2.0
DEFAULT_GAS_PRICE=20

# Monitoring
HEALTH_CHECK_INTERVAL=30000